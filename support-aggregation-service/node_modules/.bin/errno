#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/sglantz/git/crm-service/customer-relationship-management-service/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/mnt/c/Users/sglantz/git/crm-service/customer-relationship-management-service/node_modules/.pnpm/errno@0.1.8/node_modules:/mnt/c/Users/sglantz/git/crm-service/customer-relationship-management-service/node_modules/.pnpm/node_modules:/mnt/c/Users/sglantz/git/crm-service/customer-relationship-management-service/node_modules:/mnt/c/Users/sglantz/git/crm-service/node_modules:/mnt/c/Users/sglantz/git/node_modules:/mnt/c/Users/sglantz/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/sglantz/git/crm-service/customer-relationship-management-service/node_modules/errno/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/sglantz/git/crm-service/customer-relationship-management-service/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/mnt/c/Users/sglantz/git/crm-service/customer-relationship-management-service/node_modules/.pnpm/errno@0.1.8/node_modules:/mnt/c/Users/sglantz/git/crm-service/customer-relationship-management-service/node_modules/.pnpm/node_modules:/mnt/c/Users/sglantz/git/crm-service/customer-relationship-management-service/node_modules:/mnt/c/Users/sglantz/git/crm-service/node_modules:/mnt/c/Users/sglantz/git/node_modules:/mnt/c/Users/sglantz/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/sglantz/git/crm-service/customer-relationship-management-service/node_modules/errno/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../errno/cli.js" "$@"
else
  exec node  "$basedir/../errno/cli.js" "$@"
fi
